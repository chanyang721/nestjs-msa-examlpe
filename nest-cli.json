{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/project/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/project/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/project",
  "projects": {
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication/tsconfig.app.json"
      }
    },
    "configuration": {
      "type": "library",
      "root": "libs/configuration",
      "entryFile": "index",
      "sourceRoot": "libs/configuration/src",
      "compilerOptions": {
        "tsConfigPath": "libs/configuration/tsconfig.lib.json"
      }
    },
    "core-fundamentals": {
      "type": "library",
      "root": "libs/core-fundamentals",
      "entryFile": "index",
      "sourceRoot": "libs/core-fundamentals/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core-fundamentals/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "jwt": {
      "type": "library",
      "root": "libs/jwt",
      "entryFile": "index",
      "sourceRoot": "libs/jwt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/jwt/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  }
}